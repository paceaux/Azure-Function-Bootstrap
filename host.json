{
  "version": "2.0",
  "functions": ["HttpTriggerJob", "QueueTriggerJob, HttpTriggerInvokeProcedure, TimerTriggerJob"],
  "functionTimeout": "16:00:00",
  "extensions": {
    "queues": {
      "batchSize": 1,
      "maxPollingInterval": "00:00:10",
      "maxDequeueCount": 1,
      "visibilityTimeout": "00:00:15"
    }
  },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Information"
    },
    "console": {
      "isEnabled": "false"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5
      }
    }
  },
  "healthMonitor": {
    "enabled": true,
    "healthCheckInterval": "00:00:10",
    "healthCheckWindow": "00:02:00",
    "healthCheckThreshold": 6,
    "counterThreshold": 0.80
  }, 
  "watchDirectories": ["Common", "config"]
}